static void Player_actor_Set_force_shadow_position_fromAnimePosition(ACTOR* actorx) {
    PLAYER_ACTOR* player = (PLAYER_ACTOR*)actorx;
    cKF_SkeletonInfo_R_c* kf0 = &player->keyframe0;
