static void Player_actor_SetupItem_Base0(ACTOR* actorx, int anim1_idx, int item_anim_idx, f32 item_anim_speed,
                                         f32 item_morph_speed, f32 start_frame, int* anim1_idx_p,
                                         int* part_table_idx_p) {
