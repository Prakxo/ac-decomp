                Common_Get(clip).bg_item_clip->item_tree_fruit_drop_proc(item, tree_ut_x, tree_ut_z, &item_drop_pos);
                if (item_drop_pos.x >= 0.0f && item_drop_pos.z >= 0.0f) {
                    if (angle_y_p != NULL) {
