        int* shake_ut_z_p = player->shake_tree_ut_z;
        int i;

        for (i = 0; i < 3; i++) {
            if (*shake_timer_p > 0.0f && *shake_ut_x_p == tree_ut_x && *shake_ut_z_p == tree_ut_z) {
                return i;
            }

            shake_timer_p++;
            shake_ut_x_p++;
            shake_ut_z_p++;
        }
    }

    return -1;
}

