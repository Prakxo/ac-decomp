    return FALSE;
}

static int Player_actor_Check_tree_shaken(ACTOR* actorx, const xyz_t* pos_p) {
    if (Player_actor_Check_tree_shaken_little(actorx, pos_p) || Player_actor_Check_tree_shaken_big(actorx, pos_p)) {
        return TRUE;
    }

    return FALSE;
}

