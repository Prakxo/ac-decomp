        if (Player_actor_Check_HitAxe(actorx, &axe_pos)) {
            xyz_t ut_center_pos;

            if (mFI_Wpos2UtCenterWpos(&ut_center_pos, *pos_p)) {
                if (axe_pos.x == ut_center_pos.x && axe_pos.z == ut_center_pos.z) {
                    return TRUE;
                }
            }
