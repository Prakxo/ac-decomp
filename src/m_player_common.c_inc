                                                                mPlayer_REQUEST_PRIORITY_5);
            }
        }
    }

    return FALSE;
}

static int Player_actor_Get_tree_shaken_table_index(ACTOR* actorx, const xyz_t* pos_p) {
    int tree_ut_x;
    int tree_ut_z;

    if (mFI_Wpos2UtNum(&tree_ut_x, &tree_ut_z, *pos_p)) {
